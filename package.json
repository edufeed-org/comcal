{
  "name": "comcal",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --environment node",
    "test:component": "vitest run --environment jsdom",
    "test:e2e": "playwright test",
    "machine-translate": "inlang machine translate --project project.inlang"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.4.0",
    "@inlang/paraglide-sveltekit": "^0.16.1",
    "@playwright/test": "^1.58.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "daisyui": "^5.0.54",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.18",
    "ws": "^8.19.0"
  },
  "dependencies": {
    "amb-nostr-converter": "https://git.edufeed.org/api/packages/edufeed/npm/amb-nostr-converter/-/0.0.1/amb-nostr-converter-0.0.1.tgz",
    "applesauce-accounts": "^5.1.0",
    "applesauce-common": "^5.1.0",
    "applesauce-content": "^5.0.0",
    "applesauce-core": "^5.1.0",
    "applesauce-loaders": "^5.1.0",
    "applesauce-relay": "^5.1.0",
    "applesauce-signers": "^5.1.0",
    "blossom-client-sdk": "^4.1.0",
    "dompurify": "^3.3.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-svelte": "^8.6.0",
    "maplibre-gl": "^4.7.1",
    "marked": "^16.4.0",
    "nostr-tools": "^2.16.2",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.2",
    "svelte-maplibre": "^1.2.3"
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": "prettier --write"
  }
}
